# Woodpecker MCP Server Environment Variables

# Woodpecker CI server URL
WOODPECKER_MCP_WOODPECKER_URL=https://woodpecker.example.com

# Personal Access Token from Woodpecker CI
WOODPECKER_MCP_WOODPECKER_TOKEN=your-personal-access-token-here

# Logging configuration
WOODPECKER_MCP_LOGGING_LEVEL=info
WOODPECKER_MCP_LOGGING_FORMAT=text

# Server configuration
WOODPECKER_MCP_SERVER_NAME=woodpecker-mcp
WOODPECKER_MCP_SERVER_VERSION=1.0.0